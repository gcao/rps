{
  "rulesDirectory": [
    "node_modules/tslint-eslint-rules/dist/rules"
  ],
  "rules": {
    "class-name": true,
    "curly": true,
    "indent": [
      true,
      "spaces"
    ],
    "member-ordering": [
      true,
      {
        "order": "fields-first"
      }
    ],
    "new-parens": true,
    "no-angle-bracket-type-assertion": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-debugger": true,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-internal-module": true,
    "no-shadowed-variable": true,
    "no-string-literal": true,
    "no-trailing-whitespace": true,
    "no-unused-variable": [
      true,
      "react"
    ],
    "no-var-keyword": true,
    "object-curly-spacing": ["always"],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-else",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "typedef-whitespace": [
      true,
      {
        "call-signature": "nospace",
        "index-signature": "nospace",
        "parameter": "nospace",
        "property-declaration": "nospace",
        "variable-declaration": "nospace"
      }
    ],
    "variable-name": [
      true,
      "ban-keywords",
      "check-format",
      "allow-leading-underscore"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  },
  "jsRules": {
    "curly": true,
    "indent": [
      true,
      "spaces"
    ],
    "new-parens": true,
    "no-bitwise": true,
    "no-conditional-assignment": true,
    "no-consecutive-blank-lines": [
      true,
      2
    ],
    "no-debugger": false,
    "no-duplicate-variable": true,
    "no-eval": true,
    "no-shadowed-variable": true,
    "no-trailing-whitespace": true,
    "no-unused-variable": [
      true
    ],
    "object-literal-key-quotes": [
      true,
      "as-needed"
    ],
    "one-line": [
      true,
      "check-open-brace",
      "check-whitespace"
    ],
    "quotemark": [
      true,
      "single"
    ],
    "semicolon": [
      true,
      "never"
    ],
    "triple-equals": [
      true,
      "allow-null-check"
    ],
    "variable-name": [
      true,
      "ban-keywords"
    ],
    "whitespace": [
      true,
      "check-branch",
      "check-decl",
      "check-operator",
      "check-separator",
      "check-type"
    ]
  }
}